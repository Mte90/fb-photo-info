{
  "description": "Show the data find by FaceBook in photos",
  "manifest_version": 2,
  "name": "FB Photo Info",
  "version": "1.0",
  "homepage_url": "https://github.com/mte90/fb-photi-info",
  "icons": {
    "48": "icons/48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["fb.js"]
    }
  ] 
}
